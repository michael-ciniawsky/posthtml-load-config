{
  "name": "load-config-test",
  "version": "1.0.0-alpha",
  "private": true,
  "description": "Autoload Plugins for PostHTML",
  "main": "index.js",
  "dependencies": {
    "posthtml": "^0.8.7",
    "posthtml-bem": "^0.2.2",
    "posthtml-import": "^0.8.0"
  },
  "devDependencies": {},
  "posthtml": {
    "sync": false,
    "skipParse": false,
    "plugins": {
      "posthtml-import": null,
      "posthtml-bem": {
        "elemPrefix": "__",
        "modPrefix": "--",
        "modDlmtr": "-"
      }
    }
  }
}
